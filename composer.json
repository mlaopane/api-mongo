{
    "name": "mykeon/api",
    "description": "API with Slim 3 & MongoDB",
    "license": "MIT",
    "authors": [
        {
            "name": "Mickael LAO-PANE",
            "email": "mlaopane@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^3.9",
        "mongodb/mongodb": "^1.2",
        "psr/simple-cache": "^1.0",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb-odm": "^1.2",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/console": "^4.0"
    },
    "provide": {
        "ext-mongo": "1.6.14"
    },
    "autoload": {
        "psr-4": {
            "MykeOn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
